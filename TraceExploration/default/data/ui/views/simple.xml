<dashboard stylesheet="simple/bootstrap.min.css" script="simple/script.js" version="1.1" theme="dark">
    <label>Simple</label>
    <row>
        <panel>
            <html>
                <div class="container">
                    <form class="form-inline">
                        <div class="form-group">
                            <label for="application">Application:</label>
                            <input type="text" class="form-control w-80 ml-3" id="application"/>
                        </div>
                        <div class="form-group">
                            <label for="traceID">TraceID:</label>
                            <input type="text" class="form-control w-80 ml-3" id="traceID"/>
                        </div>
                        <button id="search" type="button" class="btn btn-success">Search</button>
                    </form>
                    <ul class="list-group" id="group">
                    </ul>
                </div>
            </html>
        </panel>
    </row>
</dashboard>