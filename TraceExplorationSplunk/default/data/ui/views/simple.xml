<dashboard stylesheet="simple/bootstrap.min.css" script="simple/script.js" version="1.1">
    <label>Simple</label>
    <row>
        <panel>
            <html>
                <div class="container">
                    <form class="form-inline">
                        <div class="form-group">
                            <label for="application">Application:</label>
                            <input type="text" class="form-control" id="application"/>
                        </div>
                        <div class="form-group">
                            <label for="traceID">TraceID:</label>
                            <input type="text" class="form-control" id="traceID"/>
